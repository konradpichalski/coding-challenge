{
  "name": "quotesCompare",
  "version": "1.0.0",
  "description": "A small HTML + CSS build for comparing quotes using SASS",
  "main": "index.js",
  "author": "Konrad Pichalski",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "live-server": "^1.2.1"
  },
  "scripts": {
    "start": "concurrently \"yarn server\" \"yarn sass\"",
    "server": "live-server build",
    "sass": "node-sass --watch styles -o build"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "node-sass": "^4.14.1"
  }
}
